

@model List<ADProjGp2_LogicUStationeryStore.Models.DisbursementHistoryViewModel>
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Disbursement List Summary</title>
    <link href="~/Content/css/Button-Antiman.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/Requisition.css" rel="stylesheet" type="text/css" />

</head>
<body>
<h1>Disbursement List Summary</h1>
        @using (@Html.BeginForm())
        {
            <div>
                <table id="RDlist">
                    <tr>
                        <th>Retrieval ID </th>
                        <th>Disburse Date</th>
                        <th>Disbursement ID</th>
                    </tr>


                    @for (int i = 0; i < Model.Count; i++)
                    {
                        <tr>
                            <td>@Html.ActionLink(Model[i].retrievalID, "DisbursementListDetail", new { retrievalID = Model[i].retrievalID }, new { @style = "text-decoration:underline;" })</td>
                            @Html.HiddenFor(m => m[i].retrievalID)
                            <td>@Model[i].disburseDate</td>
                            @Html.HiddenFor(m => m[i].disburseDate)
                            <td>
                                @foreach (string x in Model[i].disbursementIDs)
                                {
                                    @x;
                                    @*@Html.ActionLink(x, "DisbursementDetail", new { disburseID = x }, new { @style = "text-decoration:underline;" })*@
                                    <br />
                                }
                                @Html.HiddenFor(m => m[i].disbursementIDs)
                            </td>
                        </tr>
                    }
                </table>
            </div>
        }


</body>
</html>
